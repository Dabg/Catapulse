    <!-- Checkbox with 3 states -->
    <style type="text/css" >
      .cb_on  {
            background: url(/static/images/3states/checked.gif) no-repeat;
            cursor:pointer;
            border: none;
            display: block;
       }

      .cb_off  {
            background: url(/static/images/3states/unchecked.gif) no-repeat;
            cursor:pointer;
            border: none;
            display: block;
       }

      .cb_mute  {
            background: url(/static/images/3states/intermediate.gif) no-repeat;
            cursor:pointer;
            border: none;
            display: block;
      }
    </style>


    <!-- define the toggle/cycle function -->
     <script language="javascript">
        function toggleState(item){
        if(item.className == "cb_mute") {
              item.className="cb_on";
              item.value="1";
              item.checked=true;
        } else if(item.className == "cb_on") {
              item.className="cb_off";
              item.value="0";
              item.checked=false;
        } else {
              item.className="cb_mute";
              item.value="";
              item.indeterminate=true;
           }
        }
     </script>

